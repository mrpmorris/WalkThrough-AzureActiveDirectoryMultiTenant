{
	"AzureAd": {
		"Instance": "https://login.microsoftonline.com/",
		"TenantId": "organizations",
		"ClientId": "{Application (client) ID}",
		"Scopes": "api://{Application (client) ID}/access_as_user"
	},
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft.AspNetCore": "Warning"
		}
	},
	"AllowedHosts": "*"
}
